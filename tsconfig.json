{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "ProductsTable.testtt.tsx", "ProductsTable.testtt.tsx", "ProductsTable.testtt.tsx", "enzymeAdapter.d.ts", "enzymeAdapter.d.ts"],
  "references": [{ "path": "./tsconfig.node.json" }],
  "noImplicitSemicolons": true
  // "composite": true
}
